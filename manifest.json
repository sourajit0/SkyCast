{
  "manifest_version": 3,
  "name": "Weather",
  "version": "1.0",
  "description": "A weather extension that allows users to enter a city manually and display the current weather.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{abcdef12-3456-7890-abcd-ef1234567890}"
    }
  }
}